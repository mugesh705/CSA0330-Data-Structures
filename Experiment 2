#include <stdio.h>

int main() {
    int a[50], n, i;

    printf("Enter the number of elements: ");
    scanf("%d", &n);

    printf("Enter the numbers:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    printf("Result:\n");
    for (i = 0; i < n; i++) {
        if (a[i] % 2 == 0)
            printf("%d is Even\n", a[i]);
        else
            printf("%d is Odd\n", a[i]);
    }

    return 0;
}
