#include <stdio.h>

long long fact(int n) {
    if (n == 0 || n == 1)
        return 1;
    else
        return n * fact(n - 1);
}

int main() {
    int n;
    long long result;

    printf("Enter a number: ");
    scanf("%d", &n);

    result = fact(n);

    printf("Factorial = %lld", result);

    return 0;
}
