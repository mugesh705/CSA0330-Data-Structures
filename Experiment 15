#include <stdio.h>
#define SIZE 10

int main() {
    int hash[SIZE], i, n, key, index;

    // Initialize hash table
    for (i = 0; i < SIZE; i++)
        hash[i] = -1;

    printf("Enter number of keys: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter key %d: ", i + 1);
        scanf("%d", &key);

        index = key % SIZE;

        // Linear Probing
        while (hash[index] != -1) {
            index = (index + 1) % SIZE;
        }
        hash[index] = key;
    }

    printf("\nHash Table:\n");
    for (i = 0; i < SIZE; i++) {
        if (hash[i] != -1)
            printf("Index %d : %d\n", i, hash[i]);
        else
            printf("Index %d : Empty\n", i);
    }

    return 0;
}
